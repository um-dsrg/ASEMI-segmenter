{
    "featuriser": {
            "type": "histograms",
            "params": {
                    "use_voxel_value": "yes",
                    "histograms": [
                            { "radius": 1, "scale": 0, "num_bins": 32 },
                            { "radius": 1, "scale": 4, "num_bins": 32 }
                        ]
                }
        },
    "classifier": {
            "type": "random_forest",
            "params": {
                    "n_estimators": 32,
                    "max_depth": 10,
                    "min_samples_leaf": 1
                }
        },
    "training_set": {
            "sample_size_per_label": 10
        }
}
