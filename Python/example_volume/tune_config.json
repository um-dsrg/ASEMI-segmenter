{
    "featuriser": {
        "type": "composite",
        "params": {
                "featuriser_list": [
                    {
                        "type": "voxel",
                        "params": {
                        }
                    },
                    {
                        "type": "histogram",
                        "params": {
                            "radius": { "min": 2, "max": 32 },
                            "scale": { "min": 0, "max": 4 },
                            "num_bins": { "min": 2, "max": 32 }
                        }
                    },
                    {
                        "type": "lbp",
                        "params": {
                            "neighbouring_dims": [1, 2],
                            "radius": { "min": 2, "max": 32 },
                            "scale": { "min": 0, "max": 4 }
                        }
                    }
                ]
            }
    },
    "classifier": {
        "type": "random_forest",
        "params": {
            "n_estimators": { "min": 1, "max": 32 },
            "max_depth": { "min": 1, "max": 32 },
            "min_samples_leaf": { "min": 1, "max": 32 }
        }
    },
    "training_set": {
        "sample_size_per_label": 10
    },
    "evaluation_set": {
        "sample_size_per_label": 10,
        "same_as_train": false
    },
    "tuning": {
        "num_iterations": 5
    }
}